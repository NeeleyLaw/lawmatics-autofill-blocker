{
  "name": "Lawmatics Autofill Blocker",
  "version": "0.1.3",
  "manifest_version": 3,
  "description": "Various Lawmatics Tools custom for NeeleyLaw",
  "update_url": "https://raw.githubusercontent.com/aneeley05/lawmatics-autofill-blocker/master/updates.xml",
  "default_locale": "en",
  "background": {
    "service_worker": "js/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "js/script.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "declarativeContent",
    "debugger",
    "contextMenus",
    "activeTab",
    "cookies",
    "idle",
    "notifications",
    "scripting",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "*://*.lawmatics.com/*"
  ]
}