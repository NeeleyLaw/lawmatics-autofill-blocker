{
  "name": "Lawmatics Autofill Blocker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Disable lawmatics contact autofill",
  "update_url": "http://nlf-srv1:3445/chromeextension/updates.xml",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://*.lawmatics.com/*"
      ],
      "js": ["js/background.js"]
    }
  ],
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "fileBrowserHandler",
    "idle",
    "notifications",
    "tabs",
    "https://*.lawmatics.com/*"
  ]
}